package com.max.idea;

public class MyArrays {
    public static void main(String[] args) {
        int [] nums = {1, 3, 5, 7, 9};
        int a = nums[0];
        nums[0] = nums[nums.length - 1];
        nums[nums.length - 1] = a;
        System.out.println(nums[(nums.length - 1) / 2] + nums[0]);
    }
    }
